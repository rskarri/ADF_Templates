{
	"name": "MongoDB_AzureSQL",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "MongoDbV2Source",
						"batchSize": 1000
					},
					"sink": {
						"type": "AzureSqlSink",
						"tableOption": "autoCreate",
						"disableMetricsCollection": false,
						"writeBehavior": "insert"
					},
					"enableStaging": false,
					"parallelCopies": 20,
					"dataIntegrationUnits": 16,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['report_id']"
								},
								"sink": {
									"name": "report_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['state_id']"
								},
								"sink": {
									"name": "state_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['state_name']"
								},
								"sink": {
									"name": "state_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['from_date']"
								},
								"sink": {
									"name": "from_date",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "$['to_date']"
								},
								"sink": {
									"name": "to_date",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "$['status']"
								},
								"sink": {
									"name": "status",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['username']"
								},
								"sink": {
									"name": "username",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['company']"
								},
								"sink": {
									"name": "company",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['company_name']"
								},
								"sink": {
									"name": "company_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['password']"
								},
								"sink": {
									"name": "password",
									"type": "String"
								}
							}
						],
						"collectionReference": ""
					}
				},
				"inputs": [
					{
						"referenceName": "DS_MongoDbV2Collection1",
						"type": "DatasetReference",
						"parameters": {
							"MD_Table_Name": {
								"value": "@pipeline().parameters.MongoDBTable_Name",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DS_AzureSqlTable_Dynmamic",
						"type": "DatasetReference",
						"parameters": {
							"SQLTableName": {
								"value": "@pipeline().parameters.SQLTable_Name",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"MongoDBTable_Name": {
				"type": "string"
			},
			"SQLTable_Name": {
				"type": "string"
			}
		},
		"annotations": []
	}
}