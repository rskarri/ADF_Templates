{
	"name": "MongoDB_AzureSQL",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "MongoDbV2Source",
						"batchSize": 1000
					},
					"sink": {
						"type": "AzureSqlSink",
						"tableOption": "autoCreate",
						"disableMetricsCollection": false,
						"writeBehavior": "insert"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['report_id']"
								},
								"sink": {
									"name": "report_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['state']"
								},
								"sink": {
									"name": "state",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['district']"
								},
								"sink": {
									"name": "district",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['retailer_id']"
								},
								"sink": {
									"name": "retailer_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['retailer_name']"
								},
								"sink": {
									"name": "retailer_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['company']"
								},
								"sink": {
									"name": "company",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['product']"
								},
								"sink": {
									"name": "product",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['plant']"
								},
								"sink": {
									"name": "plant",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['opening_balance']"
								},
								"sink": {
									"name": "opening_balance",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['stock_date']"
								},
								"sink": {
									"name": "stock_date",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "$['date_q']['$date']"
								},
								"sink": {
									"name": "date_q",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DS_MongoDbV2Collection1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_AzureSqlTable_RetailStockNew",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}